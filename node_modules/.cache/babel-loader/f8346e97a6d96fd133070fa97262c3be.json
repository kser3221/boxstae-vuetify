>\n\t\t\t\t<v-divider></v-divider>\n\t\t\t\t<v-card-text style=\"height: 600px;\">\n\t\t\t\t\t<v-tabs-items v-model=\"tab\" class=\"tab-item\">\n\t\t\t\t\t\t<v-tab-item\n\t\t\t\t\t\tv-for=\"item in tabs\"\n\t\t\t\t\t\t:key=\"item.tab\"\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<v-card flat>\n\t\t\t\t\t\t\t\t<keep-alive>\n\t\t\t\t\t\t\t\t\t<component v-bind:is=\"item.content\"></component>\n\t\t\t\t\t\t\t\t</keep-alive>\n\t\t\t\t\t\t\t</v-card>\n\t\t\t\t\t\t</v-tab-item>\n\t\t\t\t\t</v-tabs-items>\n\t\t\t\t</v-card-text>\n\t\t\t\t<v-divider></v-divider>\n\t\t\t\t<v-card-actions>\n\t\t\t\t\t<v-btn\n\t\t\t\t\t  color=\"blue darken-1\"\n\t\t\t\t\t  text\n\t\t\t\t\t  @click=\"dialog = false\"\n\t\t\t\t\t>\n\t\t\t\t\t닫 기\n\t\t\t\t\t</v-btn>\n\t\t\t\t</v-card-actions>\n\t\t\t</v-card>\n\t\t</v-dialog>\n\n\t\t<v-shell\n      :shell_input=\"send_to_terminal\"\n      :commands=\"commands\"\n      @shell_output=\"prompt\"\n    ></v-shell>\n\t</div>\n</template>\n\n<script>\nimport Info from '@/views/boxstate/Boxstate-info.vue'\nimport Alarm from '@/views/boxstate/Boxstate-alarm.vue'\nimport VPN from '@/views/boxstate/Boxstate-vpn.vue'\nimport Resource from '@/views/boxstate/Boxstate-resource.vue'\nimport Traffic from '@/views/boxstate/Boxstate-traffic.vue'\n\nexport default {\n\tname: 'BoxState',\n\t\n\tcomponents: {\n\t\tInfo,\n\t\tAlarm,\n\t\tVPN,\n\t\tResource,\n\t\tTraffic,\n\t},\n\t\n\tdata: () => ({\n\t\tdialog: false,\n\t\tpopOpen: false,\n\t\tallList: { status: true, icon: 'mdi-checkbox-blank-circle'},\n\t\tifList: { status: false, icon: 'mdi-checkbox-blank-circle-outline'},\n\t\tlineList: { status: false, icon: 'mdi-checkbox-blank-circle-outline'},\n\t\ttunList: { status: false, icon: 'mdi-checkbox-blank-circle-outline'},\n\t\tcmenu: false,\n\t\tset_cmenu: true,\n\t\tcol_edit: [\n\t\t\t{  id: 1, type: ['gname'],  text: '지점명', value: true },\n\t\t\t{  id: 2, type: ['bname'],  text: '장비명', value: true },\n\t\t\t{  id: 3, type: ['ip'],  text: 'IP', value: true },\n\t\t\t{  id: 4, type: ['lan'],  text: 'LAN', value: false },\n\t\t\t{  id: 5, type: ['alldown'],  text: '연결상태', value: true },\n\t\t\t{  id: 6, type: ['wan0_status'],  text: 'WAN0', value: true },\n\t\t\t{  id: 7, type: ['wan0_up', 'wan0_down'],  text: 'WAN0 회선품질', value: false, col: ['UP 회선품질', 'DOWN 회선품질'] },\n\t\t\t{  id: 8, type: ['wan1_status'],  text: 'WAN1', value: true },\n\t\t\t{  id: 9, type: ['wan1_up', 'wan1_down'],  text: 'WAN1 회선품질', value: false, col: ['UP 회선품질', 'DOWN 회선품질'] },\n\t\t\t{ id: 10, type: ['wan2_status'],  text: 'WAN2', value: true },\n\t\t\t{ id: 11, type: ['wan2_up', 'wan2_down'],  text: 'WAN2 회선품질', value: false, col: ['UP 회선품질', 'DOWN 회선품질'] },\n\t\t\t{ id: 12, type: ['wan3_status'],  text: 'WAN3', value: true },\n\t\t\t{ id: 13, type: ['wan3_up', 'wan3_down'],  text: 'WAN3 회선품질', value: false, col: ['UP 회선품질', 'DOWN 회선품질'] },\n\t\t\t{ id: 14, type: ['lte_ip'],  text: 'LTE IP', value: true },\n\t\t\t{ id: 15, type: ['lte_status'],  text: 'LTE 망상태', value: false },\n\t\t\t{ id: 16, type: ['lte_usim'],  text: 'LTE USIM', value: false },\n\t\t\t{ id: 17, type: ['rsrp'],  text: 'LTE 신호세기', value: false },\n\t\t\t{ id: 18, type: ['vpn_cnt'],  text: 'VPN 개수', value: true },\n\t\t\t{ id: 19, type: ['vpn_up', 'vpn_down'],  text: 'VPN 트래픽', value: false, col: ['UP 트래픽', 'DOWN 트래픽'] },\n\t\t\t{ id: 20, type: ['cpu'],  text: 'CPU', value: true },\n\t\t\t{ id: 21, type: ['mem'],  text: 'Memory', value: true },\n\t\t\t{ id: 22, type: ['ses'],  text: 'Session', value: true },\n\t\t\t{ id: 23, type: ['disk'],  text: 'Disk', value: false },\n\t\t\t{ id: 24, type: ['p1_operation1', 'p1_operation2', 'p1_operation3', 'p1_operation4'],  text: '전원컨트롤', value: false, col: ['전원상태', '전원상태', '전원상태', '전원상태'] },\n\t\t\t{ id: 25, type: ['dual_power_a', 'dual_power_b'],  text: '전원이중화', value: false, col: ['Power A', 'Power B'] },\n\t\t],\n\n\t\tlists: Array.from({ length: 15000 }).map((v, k) => ({\n\t\t\tgname: `gName${k%2}`,\n\t\t\tbname: `BOX${k}`,\n\t\t\tip: Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)),\n\t\t\tlan: Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)) + '.' + Math.floor((Math.random()*255)),\n\t\t\talldown: '정상',\n\t\t\twan0_status: 'Up',\n\t\t\twan0_up: '-',\n\t\t\twan0_down: '-',\n\t\t\twan1_status: 'Up',\n\t\t\twan1_up: '-',\n\t\t\twan1_down: '-',\n\t\t\twan2_status: 'Down',\n\t\t\twan2_up: '-',\n\t\t\twan2_down: '-',\n\t\t\twan3_status: 'Down',\n\t\t\twan3_up: '-',\n\t\t\twan3_down: '-',\n\t\t\tlte_ip: '2.2.2.2',\n\t\t\tlte_status: '-',\n\t\t\tlte_usim: '-',\n\t\t\trsrp: Math.floor((Math.random()*100)),\n\t\t\tvpn_cnt: '0/0',\n\t\t\tvpn_up: '0',\n\t\t\tvpn_down: '0',\n\t\t\tcpu: Math.floor((Math.random()*100)) + '%',\n\t\t\tmem: Math.floor((Math.random()*100)) + '%',\n\t\t\tses: Math.floor((Math.random()*100)) + '%',\n\t\t\tp1_operation1: '',\n\t\t\tp1_operation2: '',\n\t\t\tp1_operation3: '',\n\t\t\tp1_operation4: '',\n\t\t\tdual_power_a: '-',\n\t\t\tdual_power_b: '-',\n\t\t})),\n\t\theaders: [\n\t\t\t{ text: '지점명', align: 'start', value: 'gname', },\n\t\t\t{ text: '장비명', value: 'bname' },\n\t\t\t{ text: 'IP', value: 'ip' },\n\t\t\t{ text: '연결상태', value: 'alldown' },\n\t\t\t{ text: 'WAN0', value: 'wan0_status' },\n\t\t\t{ text: 'WAN1', value: 'wan1_status' },\n\t\t\t{ text: 'WAN2', value: 'wan2_status' },\n\t\t\t{ text: 'WAN3', value: 'wan3_status' },\n\t\t\t{ text: 'LTE IP', value: 'lte_ip' },\n\t\t\t{ text: 'VPN 개수', value: 'vpn_cnt' },\n\t\t\t{ text: 'CPU', value: 'cpu' },\n\t\t\t{ text: 'Memory', value: 'mem' },\n\t\t\t{ text: 'Session', value: 'ses' },\n\t\t],\n\t\tstart: 0,\n\t\ttimeout: null,\n\t\trowHeight: 24,\n\t\tperPage: 25,\n\n\t\ttab: null,\n        tabs: [\n          { tab: 1, text: '상세정보', content: 'Info' },\n          { tab: 2, text: '알람', content: 'Alarm' },\n          { tab: 3, text: 'VPN현황', content: 'VPN' },\n          { tab: 4, text: '자원사용율 추이', content: 'Resource' },\n          { tab: 5, text: '트래픽 추이', content: 'Traffic' },\n        ],\n\t\tsend_to_terminal: \"\",\n\t}),\n\tmethods: {\n\t\tonScroll(e) {\n\t\t\t// debounce if scrolling fast\n\t\t\tthis.timeout && clearTimeout(this.timeout);\n\t\t\n\t\t\tthis.timeout = setTimeout(() => {\n\t\t\t\tconst { scrollTop } = e.target;\n\t\t\t\tconst rows = Math.ceil(scrollTop / this.rowHeight);\n\t\t\n\t\t\t\tthis.start = rows + this.perPage > this.lists.length ?\n\t\t\t\tthis.lists.length - this.perPage: rows;\n\t\t\n\t\t\t\tthis.$nextTick(() => {\n\t\t\t\t\te.target.scrollTop = scrollTop;\n\t\t\t\t});\n\t\t\t }, 20);\n\t\t},\n\n\t\tcolClick(item) {\n\t\t\tconsole.log(item);\n\t\t\tvar fields = [];\n\t\t},\n\t\tcolEditClick() {\n\t\t\tthis.cmenu = false;\n\t\t},\n\t\tprompt(value) {\n\t\t\tif (value == \"node -v\") {\n\t\t\t\tthis.send_to_terminal = process.versions.node;\n\t\t\t}\n\t\t}\n\t},\n\tcomputed: {\n\t\tlistsLimited() {\n\t\t\treturn this.lists.slice(this.start, this.perPage+this.start);\n\t\t},\n\t\tstartHeight() {\n\t\t\treturn this.start * this.rowHeight - 32;\n\t\t},\n\t\tendHeight() {\n\t\t\treturn this.rowHeight * (this.lists.length - this.start);\n\t\t},\n\t},\n\twatch: {\n\t\tallList (val) {\n\t\t\tconsole.log(val);\n\t\t\tif(val.status) {\n\t\t\t\tthis.allList.icon = 'mdi-checkbox-blank-circle';\n\n\t\t\t\tthis.ifList.status = false;\n\t\t\t\tthis.lineList.status = false;\n\t\t\t\tthis.tunList.status = false;\n\t\t\t\tthis.ifList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t\tthis.lineList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t\tthis.tunList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t}\n\t\t},\n\t\tifList (val) {\n\t\t\tconsole.log(val);\n\t\t\tif(val.status) {\n\t\t\t\tthis.ifList.icon = 'mdi-checkbox-blank-circle';\n\n\t\t\t\tthis.allList.status = false;\n\t\t\t\tthis.allList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t}\n\t\t},\n\t\tlineList (val) {\n\t\t\tconsole.log(val);\n\t\t\tif(val.status) {\n\t\t\t\tthis.lineList.icon = 'mdi-checkbox-blank-circle';\n\n\t\t\t\tthis.allList.status = false;\n\t\t\t\tthis.allList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t}\n\t\t},\n\t\ttunList (val) {\n\t\t\tconsole.log(val);\n\t\t\tif(val.status) {\n\t\t\t\tthis.tunList.icon = 'mdi-checkbox-blank-circle';\n\n\t\t\t\tthis.allList.status = false;\n\t\t\t\tthis.allList.icon = 'mdi-checkbox-blank-circle-outline';\n\t\t\t}\n\t\t},\n\t}\n}\n</script>\n\n<style scoped>\n.boxstate {\n\tpadding: 20px;\n}\n#virtual-scroll-table {\n\tmax-height: 400px;\n\toverflow: auto;\n}\n</style>\n"],"sourceRoot":"src/views"}]}