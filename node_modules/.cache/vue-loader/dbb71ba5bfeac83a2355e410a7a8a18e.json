t</v-row>\n\t\t\t<div>\n\t\t\t\t<span>평균(Max) : </span>\n\t\t\t\t<label>{{ chart.avg }}{{ chart.avg_unit }} ({{ chart.max }}{{ chart.max_unit }})</label>\n\t\t\t</div>\n\t\t\t<mng-line-chart :chart-data=\"chart.data\"/>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\nimport MngLineChart from '@/components/chart/MngLineChart'\n\nexport default {\n\tname: 'BoxsateResource',\n\tcomponents: {\n\t\tMngLineChart\n\t},\n\tdata: () => ({\n\t\tcharts: [\n\t\t{\n\t\t\ttitle: 'CPU',\n\t\t\tavg: 1.04,\n\t\t\tavg_unit: '%',\n\t\t\tmax: 2.04,\n\t\t\tmax_unit: '%',\n\t\t\tsel_time:1,\n\t\t\tdata:null\n\t\t},\n\t\t{\n\t\t\ttitle: 'Memory',\n\t\t\tavg: 1.04,\n\t\t\tavg_unit: '%',\n\t\t\tmax: 2.04,\n\t\t\tmax_unit: '%',\n\t\t\tsel_time:1,\n\t\t\tdata:null\n\t\t},\n\t\t{\n\t\t\ttitle: 'Session',\n\t\t\tavg: 1.04,\n\t\t\tavg_unit: '%',\n\t\t\tmax: 2.04,\n\t\t\tmax_unit: '%',\n\t\t\tsel_time:1,\n\t\t\tdata:null\n\t\t},\n\t\t{\n\t\t\ttitle: 'Disk(로그)',\n\t\t\tavg: 1.04,\n\t\t\tavg_unit: '%',\n\t\t\tmax: 2.04,\n\t\t\tmax_unit: '%',\n\t\t\tsel_time:1,\n\t\t\tdata:null\n\t\t},\n\t\t{\n\t\t\ttitle: 'Disk(복원)',\n\t\t\tavg: 1.04,\n\t\t\tavg_unit: '%',\n\t\t\tmax: 2.04,\n\t\t\tmax_unit: '%',\n\t\t\tsel_time:1,\n\t\t\tdata:null\n\t\t},\n\t\t],\n\t\ttimes: [\n\t\t\t{ value: 1, text: '6h'},\n\t\t\t{ value: 2, text: '12h'},\n\t\t\t{ value: 3, text: '24h'},\n\t\t\t{ value: 4, text: '1Week'},\n\t\t\t{ value: 5, text: '1Month'},\n\t\t\t{ value: 6, text: '1Year'},\n\t\t],\n\t}),\n\tmethods: {\n\t\tfillData (title) {\n\t\t\tthis.charts.map((d, i) => {\n\t\t\t\tif(title) {\n\t\t\t\t\tif(title != d.title) {\n\t\t\t\t\t\treturn;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\td.data = {\n\t\t\t\t\tlabels: [this.getRandomInt(), this.getRandomInt(), this.getRandomInt(), this.getRandomInt()],\n\t\t\t\t\tdatasets: [\n\t\t\t\t\t\t{\n\t\t\t\t\t\t\tlabel: 'Data One',\n\t\t\t\t\t\t\tbackgroundColor: '#f87979',\n\t\t\t\t\t\t\tdata: [this.getRandomInt(), this.getRandomInt(), this.getRandomInt(), this.getRandomInt()]\n\t\t\t\t\t\t}\n\t\t\t\t\t]\n\t\t\t\t}\n\t\t\t});\n\t\t\t/*\n\t\t\tthis.cpuData = {\n\t\t\t\tlabels: [this.getRandomInt(), this.getRandomInt()],\n\t\t\t\tdatasets: [\n\t\t\t\t\t{\n\t\t\t\t\t\tlabel: 'Data One',\n\t   \t\t\t\t\tbackgroundColor: '#f87979',\n\t   \t\t\t\t\tdata: [this.getRandomInt(), this.getRandomInt()]\n\t\t\t\t\t}\n\t\t\t\t]\n\t\t\t}\n\t\t\t*/\n\t\t},\n\t\tgetRandomInt () {\n\t\t\treturn Math.floor(Math.random() * (50 - 5 + 1)) + 5\n\t\t}\n\t},\n\tcreated() {\n\t\tthis.fillData()\n\t},\n}\n</script>\n\n<style scoped>\n.small {\n\tdisplay: inline-block;\n\twidth: 30%;\n\tmargin: 10px;\n}\n</style>\n"]}]}